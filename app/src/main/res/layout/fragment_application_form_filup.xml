<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="viewModel"
            type="com.service.appdev.coursedetails.viewmodel.ApplicationFormViewModel" />
    </data>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<TextView
    android:layout_width="match_parent"
    android:layout_height="40sp"
    android:gravity="center"
    android:layout_marginTop="16dp"
    android:id="@+id/header"
    android:textStyle="bold"
    android:textSize="20sp"
    android:textColor="#000000"
    android:text="@string/application_filup"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/student_name"
        android:text="@string/student_name"
        android:gravity="center"
        android:layout_margin="16sp"
        android:layout_below="@+id/header"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:orientation="horizontal"
        android:weightSum="2"
        android:id="@+id/student_full_name"
        android:layout_below="@+id/student_name">

        <EditText
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:importantForAutofill="no"
            android:inputType="text"
            android:id="@+id/student_first_name"
            android:background="@drawable/edittext_bg"
            android:hint="@string/first_name"/>
        <EditText
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:id="@+id/student_last_name"
            android:hint="@string/last_name"
            android:importantForAutofill="no"
            android:layout_marginStart="8dp"
            android:background="@drawable/edittext_bg"
            android:inputType="text" />

    </LinearLayout>

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/parent_name"
    android:text="@string/parent_name"
    android:gravity="center"
    android:layout_margin="16sp"
    android:layout_below="@+id/student_full_name"/>

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="16dp"
    android:orientation="horizontal"
    android:id="@+id/parent_full_name"
    android:weightSum="2"
    android:layout_below="@+id/parent_name">

    <EditText
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:importantForAutofill="no"
        android:inputType="text"
        android:id="@+id/parent_first_name"
        android:background="@drawable/edittext_bg"
        android:hint="@string/father_first_name"/>
    <EditText
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:hint="@string/fathers_last_name"
        android:id="@+id/fathers_last_name"
        android:importantForAutofill="no"
        android:layout_marginStart="8dp"
        android:background="@drawable/edittext_bg"
        android:inputType="text" />

</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:orientation="horizontal"
        android:id="@+id/parent_full_name2"
        android:weightSum="2"
        android:layout_below="@+id/parent_full_name">

        <EditText
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:importantForAutofill="no"
            android:inputType="text"
            android:id="@+id/parent_first_name2"
            android:background="@drawable/edittext_bg"
            android:hint="@string/mothers_first_name"/>
        <EditText
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:hint="@string/mothers_last_name"
            android:id="@+id/parent_last_name2"
            android:importantForAutofill="no"
            android:layout_marginStart="8dp"
            android:background="@drawable/edittext_bg"
            android:inputType="text" />

    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/student_email"
        android:layout_margin="16dp"
        android:id="@+id/student_email"
        android:layout_below="@+id/parent_full_name2"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:hint="@string/student_email_hint"
        android:importantForAutofill="no"
        android:background="@drawable/edittext_bg"
        android:layout_below="@+id/student_email"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:id="@+id/student_email_et"
        android:inputType="text" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/aadhar_no"
        android:layout_margin="16dp"
        android:id="@+id/aadhar_number_text"
        android:layout_below="@+id/student_email_et"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:hint="@string/aadhar_no"
        android:importantForAutofill="no"
        android:background="@drawable/edittext_bg"
        android:layout_below="@+id/aadhar_number_text"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:id="@+id/aadhar_number"
        android:inputType="number" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/twelfth_marks"
        android:layout_margin="16dp"
        android:id="@+id/twelfth_marks"
        android:layout_below="@+id/aadhar_number"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:hint="@string/twelfth_marks"
        android:importantForAutofill="no"
        android:background="@drawable/edittext_bg"
        android:layout_below="@+id/twelfth_marks"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:id="@+id/full_twelfth_marks"
        android:inputType="number" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/tenth_marks"
        android:layout_margin="16dp"
        android:id="@+id/tenth_marks"
        android:layout_below="@+id/full_twelfth_marks"/>

    <EditText
        android:id="@+id/full_tenth_marks"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/tenth_marks"
        android:layout_marginStart="16dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/edittext_bg"
        android:hint="@string/tenth_marks"
        android:importantForAutofill="no"
        android:inputType="number" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/joint_marks"
        android:layout_margin="16dp"
        android:id="@+id/joint_marks"
        android:layout_below="@+id/full_tenth_marks"/>
    <Spinner
        android:id="@+id/my_spinner"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:spinnerMode="dropdown"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"

        android:layout_below="@id/joint_marks"
        android:drawableEnd="@drawable/ic_down_arrow"
        android:entries="@array/dropdown_items" />

    <EditText
        android:id="@+id/full_joint_marks"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_below="@+id/my_spinner"
        android:layout_marginStart="16dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/edittext_bg"
        android:hint="@string/joint_full_marks"
        android:importantForAutofill="no"
        android:inputType="number" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/home_address"
        android:text="@string/home_address"
        android:gravity="center"
        android:layout_margin="16sp"
        android:layout_below="@+id/full_joint_marks"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:orientation="vertical"
        android:id="@+id/home_address_container"
        android:layout_below="@+id/home_address">
        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="@string/street_name"
            android:importantForAutofill="no"
            android:background="@drawable/edittext_bg"
            android:id="@+id/street_address1"
            android:inputType="text" />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="@string/street_address"
            android:importantForAutofill="no"
            android:background="@drawable/edittext_bg"
            android:id="@+id/street_address2"
            android:layout_marginTop="8dp"
            android:inputType="text" />
      <LinearLayout
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:weightSum="2"
          android:layout_marginTop="8dp"
          android:orientation="horizontal">

        <EditText
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:importantForAutofill="no"
            android:inputType="text"
            android:id="@+id/city"
            android:background="@drawable/edittext_bg"
            android:hint="@string/city_name"/>
        <EditText
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:id="@+id/state"
            android:hint="@string/state_name"
            android:importantForAutofill="no"
            android:layout_marginStart="8dp"
            android:background="@drawable/edittext_bg"
            android:inputType="text" />

      </LinearLayout>
        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="@string/pin_code"
            android:importantForAutofill="no"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittext_bg"
            android:id="@+id/pincode"
            android:inputType="number" />

    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/phone_number"
        android:layout_margin="16dp"
        android:id="@+id/phone_number"
        android:layout_below="@+id/home_address_container"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:hint="@string/phone_number_input"
        android:importantForAutofill="no"
        android:background="@drawable/edittext_bg"
        android:layout_below="@+id/phone_number"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:id="@+id/phone_number_input"
        android:inputType="phone" />



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/course_willing_1"
        android:layout_margin="16dp"
        android:id="@+id/course_willing"
        android:layout_below="@+id/phone_number_input"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder"
        android:layout_below="@+id/course_willing">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/course_willing_input"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/college_willing"
        android:layout_margin="16dp"
        android:id="@+id/college_willing"
        android:layout_below="@+id/spinnerHolder"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder1"
        android:layout_below="@+id/college_willing">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:clickable="true"
            android:importantForAccessibility="yes"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/college_willing_input"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/course_willing_2"
        android:layout_margin="16dp"
        android:id="@+id/course_willing2"
        android:layout_below="@+id/spinnerHolder1"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder2"
        android:layout_below="@+id/course_willing2">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/course_willing_input2"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/college_willing"
        android:layout_margin="16dp"
        android:id="@+id/college_willing_2"
        android:layout_below="@+id/spinnerHolder2"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder3"
        android:layout_below="@+id/college_willing_2">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:clickable="true"
            android:importantForAccessibility="yes"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/college_willing_input2"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/course_willing_3"
        android:layout_margin="16dp"
        android:id="@+id/course_willing3"
        android:layout_below="@+id/spinnerHolder3"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder4"
        android:layout_below="@+id/course_willing3">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/course_willing_input3"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/college_willing"
        android:layout_margin="16dp"
        android:id="@+id/college_willing_3"
        android:layout_below="@+id/spinnerHolder4"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder5"
        android:layout_below="@+id/college_willing_3">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:clickable="true"
            android:importantForAccessibility="yes"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/college_willing_input4"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/course_willing_4"
        android:layout_margin="16dp"
        android:id="@+id/course_willing4"
        android:layout_below="@+id/spinnerHolder5"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder6"
        android:layout_below="@+id/course_willing4">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/course_willing_input4"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/college_willing"
        android:layout_margin="16dp"
        android:id="@+id/college_willing_4"
        android:layout_below="@+id/spinnerHolder6"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder7"
        android:layout_below="@+id/college_willing_4">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:clickable="true"
            android:importantForAccessibility="yes"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/college_willing_input5"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/course_willing_5"
        android:layout_margin="16dp"
        android:id="@+id/course_willing5"
        android:layout_below="@+id/spinnerHolder7"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder8"
        android:layout_below="@+id/course_willing5">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/course_willing_input5"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/college_willing"
        android:layout_margin="16dp"
        android:id="@+id/college_willing_5"
        android:layout_below="@+id/spinnerHolder8"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/spinnerHolder9"
        android:layout_below="@+id/college_willing_5">
        <Spinner
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="Select"
            android:importantForAutofill="no"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:clickable="true"
            android:importantForAccessibility="yes"
            android:dropDownSelector="@color/purple_500"
            android:id="@+id/college_willing_input6"
            android:spinnerMode="dropdown"
            android:background="@drawable/spinner_bg"
            android:popupBackground="@drawable/your_popup_background"
            android:drawableEnd="@drawable/ic_down_arrow" />
    </LinearLayout>


    <TextView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/selectedCoursesText"
    android:text="@string/selected_courses"
    android:layout_below="@+id/spinnerHolder9"
    android:layout_margin="16sp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:minHeight="72sp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/selectedCoursesText"
        android:id="@+id/linearLayout"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/qrprompt"
        android:layout_below="@+id/linearLayout"
        android:layout_margin="16sp"
        android:text="Please scan the QR code to Pay!"/>
    <ImageView
        android:layout_width="512px"
        android:layout_height="512px"
        android:id="@+id/qrcode"
        android:layout_margin="16sp"
        android:layout_centerInParent="true"
        android:layout_below="@+id/qrprompt"
        android:src="@drawable/qrscan"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="@string/upload_receipt"
        android:background="@color/purple_200"
        android:textColor="@color/white"
        android:layout_gravity="center"
        android:id="@+id/btnSelectReceipt"
        android:layout_below="@+id/qrcode"
        android:layout_centerInParent="true"
        android:layout_margin="64dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnSelectReceipt"
        android:id="@+id/verificationStatus"
        android:layout_marginLeft="32sp"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="16sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tenth_qualification"
        android:layout_margin="16sp"
        android:layout_below="@+id/verificationStatus"
        android:id="@+id/tenthqualification"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/tenthqualification"
        android:id="@+id/tenth_qualification_holder">
        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:hint="@string/school_name"
            android:importantForAutofill="no"
            android:background="@drawable/edittext_bg"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:id="@+id/school_name_et"
            android:layout_margin="16sp"
            android:inputType="text" />
